{
  "name": "@brushy/di",
  "version": "1.0.3",
  "description": "Advanced dependency injection library with special support for React",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "authors": [
    {
      "name": "Gustavo Franco",
      "email": "contact@gfrancodev.com",
      "url": "https://github.com/gfrancodev"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./core/*": {
      "types": "./dist/core/*.d.ts",
      "import": "./dist/core/*.js",
      "require": "./dist/core/*.js"
    },
    "./lib/tools/*": {
      "types": "./dist/lib/tools/*.d.ts",
      "import": "./dist/lib/tools/*.js",
      "require": "./dist/lib/tools/*.js"
    },
    "./lib/web/*": {
      "types": "./dist/lib/web/*.d.ts",
      "import": "./dist/lib/web/*.js",
      "require": "./dist/lib/web/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "check-types": "tsc --noEmit",
    "format": "prettier . --write",
    "docs": "serve docs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "dependency-injection",
    "di",
    "ioc",
    "typescript",
    "react"
  ],
  "author": "Gustavo Franco <contact@gfrancodev.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brushysuite/brushy-librarys.git",
    "directory": "packages/di"
  },
  "bugs": {
    "url": "https://github.com/brushysuite/brushy-librarys/issues"
  },
  "homepage": "https://gfrancodev.com",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.13.9",
    "@types/react": "19.0.10",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.8",
    "@vitest/ui": "^3.0.8",
    "eslint": "^9.21.0",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "3.5.3",
    "serve": "^14.2.1",
    "typescript": "5.8.2",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
