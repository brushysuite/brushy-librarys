{
  "name": "@brushy/localstorage",
  "version": "1.0.0",
  "description": "Powerful abstraction for local state management",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "authors": [
    {
      "name": "Gustavo Franco",
      "email": "contact@gfrancodev.com",
      "url": "https://github.com/gfrancodev"
    }
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "check-types": "tsc --noEmit",
    "format": "prettier . --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "localstorage",
    "storage",
    "browser",
    "typescript",
    "compression",
    "ttl"
  ],
  "author": "Gustavo Franco <contact@gfrancodev.com>",
  "license": "MIT",
  "homepage": "https://gfrancodev.com",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brushysuite/brushy-librarys"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.8.0",
    "@types/node-localstorage": "^1.3.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.51.0",
    "jsdom": "^22.1.0",
    "node-localstorage": "^3.0.5",
    "prettier": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^19.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
  }
}
